{
    "name": "@pankod/next-boilerplate",
    "description": "A well-structured production ready Next.js boilerplate with Typescript, Redux, Jest, Enzyme, Express.js, Sass, Css, EnvConfig, Fetch, Reverse Proxy, Bundle Analyzer and Built-in Project CLI. https://pankod.github.io/next-boilerplate/",
    "version": "2.0.0",
    "repository": "https://github.com/pankod/next-boilerplate",
    "author": "Pankod <info@pankod.com>",
    "contributors": [
        "Batuhan Wilhelm <me@batuhan.dev>"
    ],
    "scripts": {
        "start:dev": "nodemon",
        "build": "next build && tsc --project tsconfig.server.json",
        "start": "next start -p $PORT",
        "test": "jest --coverage",
        "lint": "eslint './{src,pages,server}/**/*' --ext .tsx,.ts",
        "pretty": "prettier --write --config .prettierrc.js {src,pages,server}/**/*.{ts,tsx}",
        "format": "npm run lint -- --fix",
        "analyze": "cross-env BUNDLE_ANALYZE=both npm run build",
        "export": "cross-env NODE_ENV=production next export",
        "cli": "pankod-cli add",
        "heroku-postbuild": "npm run build"
    },
    "dependencies": {
        "@zeit/next-bundle-analyzer": "^0.1.2",
        "@zeit/next-css": "^1.0.1",
        "@zeit/next-sass": "^1.0.1",
        "autoprefixer": "^9.7.6",
        "cross-env": "^5.2.1",
        "dotenv": "^8.2.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "express": "^4.17.1",
        "http-proxy-middleware": "^0.20.0",
        "isomorphic-unfetch": "^3.0.0",
        "kind-of": "^6.0.3",
        "next": "^9.3.4",
        "next-compose-plugins": "^2.2.0",
        "next-i18next": "^4.2.1",
        "next-redux-wrapper": "^3.0.0",
        "next-routes": "^1.4.2",
        "next-runtime-dotenv": "^1.2.0",
        "node-sass": "^4.13.1",
        "query-string": "^6.12.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-redux": "^7.2.0",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0",
        "styled-components": "^4.4.1",
        "webpack-bundle-analyzer": "^3.3.2"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@pankod/pankod-cli": "^1.0.1",
        "@types/dotenv": "^6.1.1",
        "@types/enzyme": "^3.10.5",
        "@types/enzyme-adapter-react-16": "^1.0.6",
        "@types/express": "^4.17.5",
        "@types/jest": "^24.9.1",
        "@types/next-redux-wrapper": "^2.0.2",
        "@types/nock": "^10.0.3",
        "@types/node": "^12.12.35",
        "@types/react": "^16.9.33",
        "@types/react-redux": "^7.1.7",
        "@types/redux-mock-store": "^1.0.2",
        "@types/styled-components": "^4.4.3",
        "@typescript-eslint/eslint-plugin": "^2.27.0",
        "@typescript-eslint/parser": "^2.27.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^24.9.0",
        "babel-plugin-module-resolver": "^3.2.0",
        "babel-plugin-styled-components": "^1.10.7",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "enzyme-to-json": "^3.4.4",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-typescript": "^4.0.1",
        "eslint-config-prettier": "^6.10.1",
        "eslint-config-react-app": "^5.2.1",
        "eslint-import-resolver-typescript": "^1.1.1",
        "eslint-plugin-flowtype": "3.x",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsx-a11y": "6.x",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "1.x",
        "jest": "^24.9.0",
        "jest-styled-components": "^6.3.4",
        "nock": "^11.9.1",
        "nodemon": "^1.19.4",
        "prettier": "1.18.2",
        "redux-devtools-extension": "^2.13.8",
        "redux-mock-store": "^1.5.4",
        "ts-jest": "^24.3.0",
        "ts-node": "^8.8.2",
        "typescript": "^3.8.3"
    },
    "pankod": {
        "projectType": "nextjs2"
    },
    "license": "ISC"
}
